{
  "name": "@adonisjs/msgpack-encoder",
  "version": "1.0.1",
  "description": "Message pack encoder to encoder and decode packages",
  "main": "index.js",
  "module": "index.js",
  "browser": "browser.js",
  "files": [
    "index.js",
    "browser.js"
  ],
  "scripts": {
    "build": "webpack --mode=production",
    "lint": "standard",
    "prepublishOnly": "npm run build",
    "pretest": "npm run lint",
    "test": "nyc japa",
    "posttest": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "websocket",
    "msgpack",
    "msgpack-lite"
  ],
  "author": "virk",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "coveralls": "^3.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "japa": "^1.0.6",
    "japa-cli": "^1.0.1",
    "nyc": "^11.4.1",
    "standard": "^11.0.0",
    "webpack": "^4.0.0-beta.2",
    "webpack-cli": "^2.0.6"
  },
  "dependencies": {
    "msgpack-lite": "^0.1.26"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standard": {
    "ignore": [
      "browser.js",
      "dist/*"
    ]
  }
}
